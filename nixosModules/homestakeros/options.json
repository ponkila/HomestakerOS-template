{
  "str_testOptions": {
    "option_1": {
      "type": "str",
      "default": "example",
      "description": "Base string option.",
      "example": "hello"
    },
    "option_2": {
      "type": "nullOr (str)",
      "default": null,
      "description": "Either null or a string.",
      "example": "world"
    },
    "option_3": {
      "type": "listOf (str)",
      "default": [],
      "description": "A list of strings.",
      "example": ["a", "b"]
    },
    "option_4": {
      "type": "attrsOf (str)",
      "default": {},
      "description": "An attribute set with string values.",
      "example": {"key": "value"}
    }
  },
  "int_testOptions": {
    "option_1": {
      "type": "int",
      "default": 42,
      "description": "Base integer option.",
      "example": 100
    },
    "option_2": {
      "type": "nullOr (int)",
      "default": null,
      "description": "Either null or an integer.",
      "example": 7
    },
    "option_3": {
      "type": "listOf (int)",
      "default": [],
      "description": "A list of integers.",
      "example": [1, 2, 3]
    },
    "option_4": {
      "type": "attrsOf (int)",
      "default": {},
      "description": "An attribute set with integer values.",
      "example": { "key": 10 }
    }
  },
  "bool_testOptions": {
    "option_1": {
      "type": "bool",
      "default": false,
      "description": "Base boolean option.",
      "example": true
    },
    "option_2": {
      "type": "nullOr (bool)",
      "default": null,
      "description": "Either null or a boolean.",
      "example": true
