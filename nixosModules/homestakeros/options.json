    "option_4": {
      "type": "attrsOf (bool)",
      "default": {},
      "description": "An attribute set with boolean values.",
      "example": { "a": true }
    }
  },
  "submodule_testOptions": {
    "option_1": {
      "type": "attrsOf (submodule)",
      "default": {},
      "description": "An attribute set where each value is a submodule.",
      "example": { "mod1": { "dummy": "value" } },
      "options": {
        "dummy": {
          "type": "str",
          "default": "value",
          "description": "A dummy nested option for the submodule.",
          "example": "value"
        }
      }
    },
    "option_2": {
      "type": "listOf (attrsOf (submodule))",
      "default": {},
      "description": "An attribute set where each value is a list of submodules.",
      "example": { "group1": [ { "mod1": { "dummy": "value" } } ] },
      "options": {
        "dummy": {
          "type": "str",
          "default": "value",
          "description": "A dummy nested option for the submodule.",
          "example": "value"
        }
      }
